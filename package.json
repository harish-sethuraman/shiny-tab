{
  "name": "shiny-tab",
  "description": "replica of vercel's flashlight tabs",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "vite",
    "vercel",
    "flashlight",
    "tabs",
    "shiny"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.package.config.ts --outDir dist && vite build --config vite.website.config.ts --outDir www --minify",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@heroicons/react": "2.0.17",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.7.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.7.2"
  }
}
